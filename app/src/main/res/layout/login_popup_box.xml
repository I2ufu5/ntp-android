<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/logoutPopupBox">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jestes zalogowany jako:" />

    <TextView
        android:id="@+id/etLoginUsername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView"
        android:text="Username" />

    <Button
        android:id="@+id/btnLoginLogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/etLoginUsername"
        android:text="Wyloguj" />

    <Button
        android:id="@+id/btnLoginCancelLogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btnLoginLogout"
        android:layout_alignBottom="@+id/btnLoginLogout"
        android:layout_toEndOf="@+id/btnLoginLogout"
        android:text="Pozostan" />


</RelativeLayout>
